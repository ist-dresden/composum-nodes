<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        jcr:primaryType="sling:OrderedFolder"
        sling:resourceType="composum/nodes/console/page">
    <runtime jcr:primaryType="nt:unstructured"
            consoleId="tools-runtime"
            parentId="tools"
            menu="{Boolean}true"
            jcr:title="Runtime"
            description="/libs/composum/nodes/system/tools/runtime/description"
            sling:resourceType="composum/nodes/console/tools/page"
            categories="[system]"
            order="{Long}300">
        <files jcr:primaryType="nt:unstructured"
                consoleId="tools-runtime-files"
                jcr:title="Files"
                description="/libs/composum/nodes/system/tools/runtime/files/description"
                sling:resourceType="composum/nodes/system/tools/runtime/files"
                categories="[system]"
                order="{Long}300"
                precondition="permission:system/runtime/files=read"/>
        <threaddump jcr:primaryType="nt:unstructured"
                consoleId="tools-runtime-threaddump"
                jcr:title="Thread Dump"
                sling:resourceType="composum/nodes/console/tools/page"
                contentSrc="/libs/composum/nodes/system/tools/runtime/threaddump.html"
                categories="[system]"
                order="{Long}900"
                precondition="[servlet:composum/nodes/system/tools/runtime/threaddump,permission:system/runtime/threads=read]"/>
    </runtime>
    <sling jcr:primaryType="nt:unstructured"
            jcr:title="Sling"
            consoleId="tools-sling"
            parentId="tools"
            order="{Long}400"
            menu="{Boolean}true"
            sling:resourceType="composum/nodes/console/tools/page"
            categories="[system]">
        <adapters jcr:primaryType="nt:unstructured"
                jcr:title="Sling Adapters"
                consoleId="tools-sling-adapters"
                order="{Long}100"
                sling:resourceType="composum/nodes/console/tools/page"
                contentSrc="/libs/composum/nodes/system/tools/sling/adapters.html"
                categories="[system]"
                precondition="[servlet:composum/nodes/system/tools/sling/adapters,permission:system/sling/adapters=read]"/>
        <requests jcr:primaryType="nt:unstructured"
                jcr:title="Recent Requests"
                consoleId="tools-sling-requests"
                order="{Long}150"
                sling:resourceType="composum/nodes/console/tools/page"
                contentSrc="/libs/composum/nodes/system/tools/sling/requests.html"
                categories="[system]"
                precondition="[servlet:composum/nodes/system/tools/sling/requests,permission:system/sling/requests=read]"/>
        <resolver jcr:primaryType="nt:unstructured"
                jcr:title="Resource Resolver"
                consoleId="tools-sling-resolver"
                order="{Long}200"
                sling:resourceType="composum/nodes/console/tools/page"
                contentSrc="/libs/composum/nodes/system/tools/sling/resolver.html"
                categories="[system]"
                precondition="[servlet:composum/nodes/system/tools/sling/resolver,permission:system/sling/resolver=read]"/>
        <servlets jcr:primaryType="nt:unstructured"
                jcr:title="Servlet Resolver"
                consoleId="tools-sling-servlets"
                order="{Long}250"
                sling:resourceType="composum/nodes/console/tools/page"
                contentSrc="/libs/composum/nodes/system/tools/sling/servlets.html"
                categories="[system]"
                precondition="[servlet:composum/nodes/system/tools/sling/servlets,permission:system/sling/servlets=read]"/>
        <jobs jcr:primaryType="nt:unstructured"
                jcr:title="Jobs / Events"
                consoleId="tools-sling-jobs"
                order="{Long}500"
                sling:resourceType="composum/nodes/console/tools/page"
                contentSrc="/libs/composum/nodes/system/tools/sling/jobs.html"
                categories="[system]"
                precondition="[servlet:composum/nodes/system/tools/sling/jobs,permission:system/sling/jobs=read]"/>
    </sling>
    <osgi jcr:primaryType="nt:unstructured"
            consoleId="tools-osgi"
            parentId="tools"
            menu="{Boolean}true"
            jcr:title="OSGi"
            description="/libs/composum/nodes/system/tools/osgi/description"
            sling:resourceType="composum/nodes/console/tools/page"
            categories="[system]"
            order="{Long}500">
        <bundles jcr:primaryType="nt:unstructured"
                consoleId="tools-osgi-bundles"
                jcr:title="Bundles"
                description="/libs/composum/nodes/system/tools/osgi/bundles/description"
                sling:resourceType="composum/nodes/system/tools/osgi/bundles"
                categories="[system]"
                order="{Long}50"
                precondition="permission:system/osgi/bundles=read"/>
    </osgi>
    <webconsole jcr:primaryType="nt:unstructured"
            consoleId="tools-webconsole"
            parentId="tools"
            jcr:title="Web Console"
            description="/libs/composum/nodes/system/tools/webconsole/description"
            sling:resourceType="composum/nodes/system/tools/webconsole"
            categories="[system]"
            order="{Long}900"
            precondition="[permission:system/webconsole=read,http:/system/console]"/>
</jcr:root>
