<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        jcr:primaryType="sling:OrderedFolder"
        sling:resourceType="composum/nodes/console/page">
    <pckgmgr jcr:primaryType="nt:unstructured"
            jcr:mixinTypes="[sling:Redirect]"
            consoleId="packages"
            jcr:title="Packages"
            description="/libs/composum/nodes/pckgmgr/description"
            sling:redirect="/bin/packages.html"
            categories="[core,nodes,packages]"
            order="{Long}30"
            permissionsSupport="{Boolean}true"
            precondition="[class:org.apache.jackrabbit.vault.packaging.JcrPackageManager,restrictions:nodes/packages/manager=read,resource:/libs/composum/nodes/pckgmgr]"/>
</jcr:root>
