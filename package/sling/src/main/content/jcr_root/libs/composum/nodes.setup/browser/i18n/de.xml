<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
        jcr:language="de"
        jcr:mixinTypes="[mix:language]"
        jcr:primaryType="sling:Folder"
        sling:basename="composum-nodes">

    <property jcr:primaryType="sling:Folder">

        <xss-confirm jcr:primaryType="sling:MessageEntry"
                sling:key="The value contains XSS stuff. A switch could trigger its execution. Do you really want to switch?"
                sling:message="Der Wert enthält XSS-Zeug. Ein Umschalten könnten dessen Ausführung auslösen. Wollen Sie wirklich umschalten?"/>

    </property>

    <components jcr:primaryType="sling:Folder">

        <overlay-path jcr:primaryType="sling:MessageEntry"
                sling:key="Overlay / Base Path" sling:message="Overlay- / Basis-Pfad"/>
        <create-overlay-title jcr:primaryType="sling:MessageEntry"
                sling:key="Create an Overlay / Base" sling:message="Ein Overlay / eine Basis anlegen"/>
        <create-overlay-text jcr:primaryType="sling:MessageEntry"
                sling:key="Do you want to create an overlay / base to the current component or copy from this template again?"
                sling:message="Wollen sie ein Overlay / eine Basis zur aktuellen Komponente anlegen bzw. erneut von dieser Vorlage kopieren?"/>
        <remove-overlay-title jcr:primaryType="sling:MessageEntry"
                sling:key="Remove the Overlay / Base" sling:message="Das Overlay / die Basis löschen"/>
        <remove-overlay-text jcr:primaryType="sling:MessageEntry"
                sling:key="Do you really want to delete the current overlay / base? This cannot be undone."
                sling:message="Wollen Sie das aktuelle Overlay / die Basis wirklich löschen; dies kann nicht rückgängig gemacht werden?"/>

    </components>

    <references jcr:primaryType="sling:Folder">

        <Search_Options jcr:primaryType="sling:MessageEntry"
                sling:key="Search Options" sling:message="Suchoptionen"/>
        <References_Options jcr:primaryType="sling:MessageEntry"
                sling:key="References Options" sling:message="Referenz-Optionen"/>
        <Apply jcr:primaryType="sling:MessageEntry"
                sling:key="Apply" sling:message="Anwenden"/>
        <Base_Path jcr:primaryType="sling:MessageEntry"
                sling:key="Base Path" sling:message="Basis-Pfad"/>
        <absolute_paths jcr:primaryType="sling:MessageEntry"
                sling:key="absolute paths" sling:message="absolute Pfade"/>
        <relative_paths jcr:primaryType="sling:MessageEntry"
                sling:key="relative paths" sling:message="relative Pfade"/>
        <include_children jcr:primaryType="sling:MessageEntry"
                sling:key="include children" sling:message="Subknoten einschließen"/>
        <children_only jcr:primaryType="sling:MessageEntry"
                sling:key="children only" sling:message="nur Subknoten"/>
        <use_text_search jcr:primaryType="sling:MessageEntry"
                sling:key="use text search" sling:message="die Textsuche verwenden"/>
        <find_rich_text jcr:primaryType="sling:MessageEntry"
                sling:key="search in rich text" sling:message="in 'rich text' suchen"/>
        <no_references jcr:primaryType="sling:MessageEntry"
                sling:key="no references found" sling:message="keine Referenzen gefunden"/>

    </references>

</jcr:root>
