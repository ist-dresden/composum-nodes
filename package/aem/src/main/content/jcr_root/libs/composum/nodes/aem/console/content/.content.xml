<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
        xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        jcr:primaryType="sling:OrderedFolder"
        sling:resourceType="composum/nodes/console/page">
    <crxde
            jcr:primaryType="nt:unstructured"
            jcr:title="CRXDE"
            sling:resourceType="composum/nodes/aem/console/components/crxde"
            description="/libs/composum/nodes/aem/console/components/crxde/description"
            categories="[aem,nodes]"
            order="{Long}20"
            target="aem"/>
    <packmgr
            jcr:primaryType="nt:unstructured"
            jcr:title="Packages"
            sling:resourceType="composum/nodes/aem/console/components/packmgr"
            description="/libs/composum/nodes/aem/console/components/packmgr/description"
            categories="[aem,nodes]"
            order="{Long}30"
            target="aem"/>
    <assets
            jcr:primaryType="nt:unstructured"
            jcr:title="Assets"
            sling:redirect="/assets.html${path}"
            pathCondition="^/content/dam(/.+)?$"
            dynamicRedirect="{Boolean}true"
            categories="[aem,nodes]"
            order="{Long}200"
            target="aem"/>
    <fragments
            jcr:primaryType="nt:unstructured"
            jcr:title="Fragments"
            sling:redirect="/aem/experience-fragments.html${path}"
            pathCondition="^/content/experience-fragments(/.+)?$"
            dynamicRedirect="{Boolean}true"
            categories="[aem,nodes]"
            order="{Long}300"
            target="aem"/>
    <sites
            jcr:primaryType="nt:unstructured"
            jcr:title="Sites"
            sling:redirect="/sites.html${path}"
            pathCondition="^/content(?!(/(dam|experience-fragments)/)).*$"
            dynamicRedirect="{Boolean}true"
            categories="[aem,nodes]"
            order="{Long}400"
            target="aem"/>
    <editor
            jcr:primaryType="nt:unstructured"
            jcr:title="Editor"
            sling:redirect="/editor.html${path.html}"
            pathCondition="^/content(?!(/(dam)/)).*$"
            dynamicRedirect="{Boolean}true"
            categories="[aem,nodes]"
            order="{Long}500"
            target="_blank"/>
</jcr:root>
